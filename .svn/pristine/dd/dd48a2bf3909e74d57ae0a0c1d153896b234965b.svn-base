package shohinmodel.bean;

import java.io.Serializable;

public class Medicine implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	// 医薬品説明文
	private String medCaption;
	// 医薬品注意事項
	private String medAttention;

	public String getMedCaption() {
		return medCaption;
	}

	public void setMedCaption(String medCaption) {
		this.medCaption = medCaption;
	}

	public String getMedAttention() {
		return medAttention;
	}

	public void setMedAttention(String medAttention) {
		this.medAttention = medAttention;
	}

}
