package shohinmodel.bean;

import java.io.Serializable;

public class Inventory implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	// 在庫数
	private int inventoryCount;
	// 項目選択肢別在庫用縦軸選択肢子番号
	private String childNoVertical;
	// 項目選択肢別在庫用横軸選択肢子番号
	private String childNoHorizontal;
	// 項目選択肢別在庫用縦軸選択肢
	private String optionNameVertical;
	// 項目選択肢別在庫用横軸選択肢
	private String optionNameHorizontal;
	// 項目選択肢別在庫用取り寄せ可能表示
	private boolean isBackorderAvailable;
	// 在庫あり時納期管理番号
	private String normalDeliveryDateId;
	// 在庫切れ時納期管理番号
	private String backorderDeliveryDateId;
	// 在庫切れ時の注文受付
	private boolean isBackorder;
	// 在庫戻し設定
	private boolean isRestoreInventoryFlag;

	public int getInventoryCount() {
		return inventoryCount;
	}

	public void setInventoryCount(int inventoryCount) {
		this.inventoryCount = inventoryCount;
	}

	public String getChildNoVertical() {
		return childNoVertical;
	}

	public void setChildNoVertical(String childNoVertical) {
		this.childNoVertical = childNoVertical;
	}

	public String getChildNoHorizontal() {
		return childNoHorizontal;
	}

	public void setChildNoHorizontal(String childNoHorizontal) {
		this.childNoHorizontal = childNoHorizontal;
	}

	public String getOptionNameVertical() {
		return optionNameVertical;
	}

	public void setOptionNameVertical(String optionNameVertical) {
		this.optionNameVertical = optionNameVertical;
	}

	public String getOptionNameHorizontal() {
		return optionNameHorizontal;
	}

	public void setOptionNameHorizontal(String optionNameHorizontal) {
		this.optionNameHorizontal = optionNameHorizontal;
	}

	public boolean isBackorderAvailable() {
		return isBackorderAvailable;
	}

	public void setBackorderAvailable(boolean isBackorderAvailable) {
		this.isBackorderAvailable = isBackorderAvailable;
	}

	public String getNormalDeliveryDateId() {
		return normalDeliveryDateId;
	}

	public void setNormalDeliveryDateId(String normalDeliveryDateId) {
		this.normalDeliveryDateId = normalDeliveryDateId;
	}

	public String getBackorderDeliveryDateId() {
		return backorderDeliveryDateId;
	}

	public void setBackorderDeliveryDateId(String backorderDeliveryDateId) {
		this.backorderDeliveryDateId = backorderDeliveryDateId;
	}

	public boolean isBackorder() {
		return isBackorder;
	}

	public void setBackorder(boolean isBackorder) {
		this.isBackorder = isBackorder;
	}

	public boolean isRestoreInventoryFlag() {
		return isRestoreInventoryFlag;
	}

	public void setRestoreInventoryFlag(boolean isRestoreInventoryFlag) {
		this.isRestoreInventoryFlag = isRestoreInventoryFlag;
	}

}
