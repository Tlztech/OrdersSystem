package shohinmodel.bean;

import java.io.Serializable;
import java.util.List;

public class ItemInventory implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	// 在庫タイプ（在庫種別）
	private int inventoryType;
	// 在庫情報リスト
	private List<Inventories> inventories;
	// 項目選択肢別在庫用縦軸選択肢項目名
	private String verticalName;
	// 項目選択肢別在庫用横軸選択肢項目名
	private String horizontalName;
	// 在庫数表示
	private int inventoryQuantityFlag;
	// 項目選択肢別在庫用残り表示閾値
	private int inventoryDisplayFlag;

	public int getInventoryType() {
		return inventoryType;
	}

	public void setInventoryType(int inventoryType) {
		this.inventoryType = inventoryType;
	}

	public List<Inventories> getInventories() {
		return inventories;
	}

	public void setInventories(List<Inventories> inventories) {
		this.inventories = inventories;
	}

	public String getVerticalName() {
		return verticalName;
	}

	public void setVerticalName(String verticalName) {
		this.verticalName = verticalName;
	}

	public String getHorizontalName() {
		return horizontalName;
	}

	public void setHorizontalName(String horizontalName) {
		this.horizontalName = horizontalName;
	}

	public int getInventoryQuantityFlag() {
		return inventoryQuantityFlag;
	}

	public void setInventoryQuantityFlag(int inventoryQuantityFlag) {
		this.inventoryQuantityFlag = inventoryQuantityFlag;
	}

	public int getInventoryDisplayFlag() {
		return inventoryDisplayFlag;
	}

	public void setInventoryDisplayFlag(int inventoryDisplayFlag) {
		this.inventoryDisplayFlag = inventoryDisplayFlag;
	}

}
